            var _fecha = $("#ddlDias").val();
            var _rangoHora = $("#ddlHoras").val();
            var _OrdenNo = $("#OrderNo").val();
            var _fechaOriginal = $("#hddFecEntrega").val();
            var _pass = $("#txtPassUpdHr").val();

            if (_pass.length == 0) {
                $("#txtPassUpdHr").focus();
                return;
            }
             $.ajax({
                async: false,
                type: "GET",
                url: '@Url.Action("ActualizaFecEntre", "Ordenes")',
                 data: {
                     fecha: _fecha
                     , rangoHora: _rangoHora
                     , OrdenNo: _OrdenNo
                     , fechaOriginal: _fechaOriginal
                     , pass: _pass
                },
                datatype: "html",
                success: function (json) {
                debugger;
                    if (json.Success) {
                        if (!alert(json.Message)) { window.location.reload(); }
                    }
                    else {
                        if (!alert(json.Message)) { $("#txtPassUpdHr").focus(); }
                    }
            }
            , error: function (jqXHR, textStatus, errorThrown) {
            bootbox.alert(errorThrown);

            }
            });
        }
