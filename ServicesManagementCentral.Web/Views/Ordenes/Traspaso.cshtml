<div class="modal inmodal fade" id="dlgTraspaso" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <div class="modal-header pt-3 pb-1">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">Traspaso</h4>
            </div>
            <div class="modal-body">
                <form id="frmCreateServicios">
                    <div class="row">
                        <div class="col col-xs-12 col-md-12">
                            <div class="form-group">
                                <label>Tienda de Origen</label>
                                <select class="form-control m-b" id="ddlTdasTrasp"></select>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col col-xs-12 col-md-12">
                            <div class="form-group">
                                <label>Número de Orden</label>
                                <select class="form-control m-b" id="ddlOrdenTraspaso">
                                   
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col col-xs-12 col-md-12">
                            <div class="form-group">
                                <p id="lblClienteTras"></p>
                                <p id="lblDireccionTras"></p>
                                <p id="lblCPTras"></p>
                                <p id="lblTelefonoTras"></p>
                            </div>
                        </div>
                    </div>
                    <div class="row">                      
                        <div class="col col-xs-6 col-md-6">
                            <div class="form-group">
                                <label>Clave</label>
                                <input type="password" id="txtPass"  class="form-control" />
                            </div>
                        </div>
                    </div>
                </form>
            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-white" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" onclick="TraspasaOrden()">Guardar</button>
            </div>
        </div>
    </div>
</div>

