            var _NumOrden = $('#ddlOrdenTraspaso').val();
            var _NumUnNva = $('#ddlTdasTrasp').val();
            var _pass = $('#txtPass').val();
            // string  = "", string  = ""

            if (_NumUnNva == "0") {
                $('#ddlTdasTrasp').focus();
                return;
            }

            if (_NumOrden == "0") {
                $('#ddlOrdenTraspaso').focus();
                return;
            }



            if (_pass.length == 0) {
                $('#txtPass').focus();
                return;
            }



            debugger;
            $.ajax({
                async: false,
                type: "POST",
                url: '@Url.Action("TraspasoOrden", "Ordenes")',
                data: {
                      NumOrden: _NumOrden
                    , NumUnNva: _NumUnNva
                    , pass: _pass
                },
                datatype: "html",
                success: function (json) {
                    debugger;
                    if (json.Success) {

                        if (!alert(json.Message)) { window.location.reload(); }
                    }
                    else {
                      alert(json.Message);
                    }
                }
                , error: function (jqXHR, textStatus, errorThrown) {
                    alert(errorThrown);

                }
            });
        }
    </script>
